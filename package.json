{
  "name": "websynth",
  "version": "1.0.0",
  "description": "WebAudio Experiments",
  "main": "app.js",
  "repository": "https://github.com/joshringer/soundscape",
  "author": "Joshua Ringer <joshringer@users.noreply.github.com>",
  "license": "Unlicense",
  "scripts": {
    "test": "standard --fix \"src/**/*.js\" *.config.js",
    "build": "standard --fix \"src/**/*.js\" *.config.js && webpack -p && postcss src/css/index.css > style.css",
    "watchcss": "postcss src/css/index.css --watch > style.css",
    "watchjs": "webpack --progress --watch",
    "start": "webpack-dev-server --open"
  },
  "devDependencies": {
    "cssnano": "^3.10.0",
    "postcss-cli": "^4.1.0",
    "standard": "*",
    "webpack": "^3.4.0",
    "webpack-dev-server": "^2.6.1"
  },
  "dependencies": {
    "jquery": "^3.2.1"
  }
}
